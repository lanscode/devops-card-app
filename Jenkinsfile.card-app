pipeline{
 agent any
 stages{
    stage("build test and package"){
       steps{
          bat "mvn clean package"
       }
       post{
         success{
            archiveArtifacts: artifacts: '**/*.war'
         }
       }
    }
    stage("Run ansible management job"){
       steps{
			        build job:'card-app-deployment'
			        }
    }

 }
}
